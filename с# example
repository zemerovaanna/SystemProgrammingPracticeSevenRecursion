using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    internal class Program
    {
        static int Rec(int n, int a)
        {
            if (a == n)
            {
                Console.WriteLine("1");
                return 1;
            }

            if (a > n)
            {
                Console.WriteLine("0");
                return 0;
            }

            return Rec(n - a, a + 1) + Rec(n, a + 1);
        }


        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int result = Rec(n, 1);
            Console.WriteLine($"Итог: {result}");
            Console.ReadKey();
        }

    }
}
